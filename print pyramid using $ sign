// Online C++ compiler to run C++ program online
#include <iostream>

int main() {
int n, q; //n lines, and q $
    for (n = 0; n <= 5; n++){ //generate n lines until to 5
        for (q = 0; q < n; q++) //generate q of '$' until q=n
        printf("$"); //print '$'
        printf("\n"); //after printing "$", new line
    }
    return 0;
}
